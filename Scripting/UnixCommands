ls -al commands.txt | cut -d '/' -f 2
sort -t <delim> -k <field>
du -sh .
svn stat | grep "^[?]" | tr -s ' ' | cut -d ' ' -f 2 | xargs rm -rf

~/.inputrc
=========
"\ep": history-search-backward
"\en": history-search-forward

gitk


update and upgrade
=========================
sudo apt-get update && sudo apt-get upgrade
